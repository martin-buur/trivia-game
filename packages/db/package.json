{
  "name": "@trivia/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx src/seed.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.3",
    "@supabase/supabase-js": "2.50.0",
    "@trivia/types": "workspace:*",
    "drizzle-orm": "0.44.2",
    "postgres": "3.4.6",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/node": "24.0.2",
    "dotenv": "16.5.0",
    "drizzle-kit": "0.30.2",
    "tsx": "4.19.2",
    "typescript": "5.8.3"
  }
}
